<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AfterLogin">
   <right>
      <AnchorPane prefHeight="800.0" prefWidth="1200.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="table" layoutX="14.0" layoutY="14.0" prefHeight="340.0" prefWidth="720.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Name" />
                  <TableColumn prefWidth="75.0" text="Qty" />
                <TableColumn prefWidth="75.0" text="Price" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <TextField fx:id="searchTextField" alignment="CENTER_RIGHT" layoutX="14.0" layoutY="354.0" prefHeight="25.0" prefWidth="522.0" promptText="search" />
            <ScrollPane fx:id="scrollPane" layoutX="734.0" layoutY="14.0" prefHeight="340.0" prefWidth="440.0">
               <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="340.0" prefWidth="430.0" />
               </content>
            </ScrollPane>
            <Button fx:id="logout" layoutX="1134.0" layoutY="761.0" mnemonicParsing="false" onAction="#logout" text="Button" />
         </children>
      </AnchorPane>
   </right>
</BorderPane>
